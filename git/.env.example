# Required (for private repos / higher rate limits)
GITHUB_TOKEN=ghp_xxx

# Optional
PORT=3000
HOST=127.0.0.1

UPSTREAM=raw.githubusercontent.com
UPSTREAM_MOBILE=raw.githubusercontent.com
UPSTREAM_PATH=/XMZO/pic/main
UPSTREAM_HTTPS=true

DISABLE_CACHE=false
# Cache-Control
# If set, overrides for all responses (when DISABLE_CACHE=false)
CACHE_CONTROL=
# Defaults by content-type (used when CACHE_CONTROL is empty)
CACHE_CONTROL_MEDIA=public, max-age=43200000
CACHE_CONTROL_TEXT=public, max-age=60

# Comma-separated lists (e.g. "CN,HK" or "1.2.3.4,5.6.7.8")
BLOCKED_REGION=
# Default (if omitted): "0.0.0.0,127.0.0.1"
BLOCKED_IP_ADDRESS=

# CORS
# "*" to allow any origin, or comma-separated allowlist like "https://a.com,https://b.com"
CORS_ORIGIN=*
# Only effective when CORS_ORIGIN is an allowlist (not "*")
CORS_ALLOW_CREDENTIALS=false
# Comma-separated list
CORS_EXPOSE_HEADERS=Accept-Ranges, Content-Length, Content-Range, ETag, Cache-Control, Last-Modified

# JSON object. Placeholders: $upstream, $custom_domain
REPLACE_DICT={"$upstream":"$custom_domain"}

# "token" (classic PAT) or "Bearer" (fine-grained tokens)
GITHUB_AUTH_SCHEME=token
upstream=
custom_domain=
